    "    \n",
    "num=int(input(\"Enter the Number:\"))    \n",
    "fact=1\n",
    "if(num<0):\n",
    "    print(\"Factorial does not exist for a negative number\")\n",
    "elif(num==0):\n",
    "    print(\"The factorial of 0 is 1\")\n",
    "for i in range(1,num+1):\n",
    "    fact=fact*i\n",
    "    print(\"The factorial of\",num,\"is\",fact)"
